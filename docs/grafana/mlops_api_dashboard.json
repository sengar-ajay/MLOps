{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Select your Prometheus data source",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
    
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.5.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "9.5.0" },
    { "type": "panel", "id": "bargauge", "name": "Bar gauge", "version": "9.5.0" },
    { "type": "panel", "id": "gauge", "name": "Gauge", "version": "9.5.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "9.5.0" }
  ],
  "title": "MLOps API Overview",
  "editable": true,
  "timezone": "browser",
  "uid": "mlops-api-overview",
  "version": 1,
  "schemaVersion": 38,
  "refresh": "10s",
  "tags": ["mlops", "api", "prometheus"],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "label": "Prometheus",
        "query": "prometheus",
        "current": {
          "selected": false
        }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "panels": [
    {
      "id": 1,
      "title": "API Requests by endpoint/method (rate)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (method, endpoint) (rate(api_requests_total[5m]))",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "title": "Requests by status (rate)",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (status) (rate(api_requests_total[5m]))",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 }
    },
    {
      "id": 3,
      "title": "Predictions by model/status (rate)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (model_type, status) (rate(ml_predictions_total[5m]))",
          "legendFormat": "{{model_type}} {{status}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 9 }
    },
    {
      "id": 4,
      "title": "P95 Prediction Latency (s)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ml_prediction_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 9 }
    },
    {
      "id": 5,
      "title": "Current Model Accuracy (Gauge)",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "min": 0, "max": 1 } },
      "targets": [
        {
          "expr": "ml_model_accuracy",
          "legendFormat": "accuracy",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 7, "w": 6, "x": 0, "y": 18 }
    },
    {
      "id": 6,
      "title": "Validation Errors by field (rate)",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum by (error_type) (rate(input_validation_errors_total[5m]))",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 7, "w": 6, "x": 6, "y": 18 }
    },
    {
      "id": 7,
      "title": "Total Predictions (last 5m)",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(increase(ml_predictions_total[5m]))",
          "legendFormat": "predictions",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 7, "w": 6, "x": 12, "y": 18 }
    }
  ]
}

